{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "target": "es6",
    "baseUrl": ".",
    "paths":  {
      "@/libs/*": ["./libs/*"],
      "@/config/*": ["./config/*"],
      "@/assets/*": ["./assets/*"],
      "@/utils/*": ["./utils/*"],
      "@/components/*": ["./components/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
